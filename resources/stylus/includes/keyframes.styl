
@keyframes fadeIn
  from
    opacity 0
  to
    opacity 1

@keyframes charIn
  from
    opacity 0
    transform perspective(200px) rotateY(90deg)
  to
    opacity 1
    transform perspective(200px) rotateY(0deg)


@keyframes spinIn
  from
    opacity 0
    transform rotate(90deg)
  to
    opacity 1
    transform rotate(0deg)

@keyframes cellIn
  from
    transform scale(0.98)
    opacity 0
  to
    transform scale(1)
    opacity 1



@keyframes cellInY
  from
    transform translate(0, 10px) scale(0.9)
    opacity 0
  to
    transform translate(0, 0) scale(1)
    opacity 1

@keyframes cellOutY
  from
    transform translate(0, -10px)
    opacity 0
  to
    transform translate(0, 0)
    opacity 1

@keyframes cellInX
  from
    transform translate(10px, 0)
    opacity 0
  to
    transform translate(0, 0)
    opacity 1

@keyframes cellOutY
  from
    transform translate(0, -10px)
    opacity 0
  to
    transform translate(0, 0)
    opacity 1

@keyframes cellOutX
  from
    transform translate(-10px, 0)
    opacity 0
  to
    transform translate(0, 0)
    opacity 1
